{
  "name": "maffin",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "amplify/backend/function/stockerlambda"
  ],
  "scripts": {
    "maffin:dev": "next dev",
    "maffin:run": "next start",
    "maffin:build": "next build",
    "maffin:test": "jest --watchAll=false",
    "maffin:lint": "yarn eslint . --ext .js,.jsx,.ts,.tsx",
    "stocker:build": "yarn workspace stockerlambda build",
    "stocker:dev": "yarn workspace stockerlambda dev",
    "stocker:lint": "yarn workspace stockerlambda lint",
    "stocker:test": "yarn workspace stockerlambda test",
    "stocker:install": "yarn workspace stockerlambda install",
    "stocker:test:events": "yarn workspace stockerlambda test:events"
  },
  "dependencies": {
    "@dinero.js/currencies": "^2.0.0-alpha.14",
    "@fontsource/inter": "^5.0.15",
    "@hookform/resolvers": "^3.3.2",
    "@tanstack/react-table": "^8.10.7",
    "@testing-library/jest-dom": "^6.1.4",
    "aws-amplify": "^5.3.12",
    "chart.js": "^4.4.0",
    "chartjs-adapter-luxon": "^1.3.1",
    "chartjs-chart-treemap": "^2.3.0",
    "chartjs-plugin-autocolors": "^0.2.2",
    "chartjs-plugin-datalabels": "^2.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.10",
    "debounce-promise": "^3.1.2",
    "dinero.js": "^2.0.0-alpha.14",
    "luxon": "^3.4.3",
    "next": "^14.0.2",
    "pako": "^2.1.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-hotkeys-hook": "^4.4.1",
    "react-icons": "^4.11.0",
    "react-joyride": "^2.7.0",
    "react-modal": "^3.16.1",
    "react-select": "^5.8.0",
    "react-tailwindcss-datepicker": "^1.6.6",
    "react-tooltip": "^5.23.0",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.32.6",
    "simplebar-react": "^3.2.4",
    "sql.js": "^1.8.0",
    "swr": "^2.2.4",
    "typeorm": "^0.3.17",
    "winston-react": "^1.0.0-RC.0"
  },
  "eslintConfig": {
    "plugins": [
      "prettier"
    ],
    "extends": [
      "next",
      "prettier",
      "airbnb",
      "airbnb-typescript",
      "react-app/jest",
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "semi": "error",
      "no-unexpected-multiline": "error",
      "arrow-parens": "off",
      "react/require-default-props": "off",
      "max-classes-per-file": "off",
      "@typescript-eslint/lines-between-class-members": "off",
      "react/no-array-index-key": "off",
      "jsx-a11y/no-static-element-interactions": "off",
      "jsx-a11y/click-events-have-key-events": "off",
      "import/prefer-default-export": "off",
      "@typescript-eslint/ban-ts-comment": "off",
      "@typescript-eslint/ban-types": "off",
      "@typescript-eslint/no-use-before-define": "off",
      "new-cap": "off",
      "jsx-a11y/label-has-associated-control": "off",
      "react/jsx-props-no-spreading": "off",
      "no-underscore-dangle": "off",
      "react/destructuring-assignment": "off",
      "import/extensions": "off",
      "no-param-reassign": "off"
    },
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "ignorePatterns": [
      "*.js",
      "*.d.ts",
      "amplify/**/*"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/debounce-promise": "^3.1.8",
    "@types/gapi": "^0.0.46",
    "@types/gapi.auth2": "^0.0.59",
    "@types/gapi.client.drive": "^3.0.15",
    "@types/gapi.client.people": "^1.0.5",
    "@types/google.accounts": "^0.0.14",
    "@types/jest": "^29.5.5",
    "@types/lodash.debounce": "^4.0.7",
    "@types/luxon": "^3.3.3",
    "@types/node": "^20.9.0",
    "@types/pako": "^2.0.3",
    "@types/react": "18.2.37",
    "@types/react-dom": "^18.2.14",
    "@types/react-modal": "^3.16.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-table": "^7.7.18",
    "@types/react-test-renderer": "^18.0.6",
    "@types/sql.js": "^1.4.9",
    "autoprefixer": "^10.4.16",
    "babel-preset-react-app": "^10.0.1",
    "eslint": "^8.53.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "^13.5.6",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.30",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "snapshot-diff": "^0.10.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "webpack-filter-warnings-plugin": "^1.2.1"
  }
}
